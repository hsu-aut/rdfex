@prefix ol: <http://www.hsu-hh.de/aut/ontologies/olif#>.

<#MfgDateMapping> a ol:DataMap;
  ol:ontologicalSource [
    ol:source "robotConfig_ABC.ttl";
    ol:sourceType ol:File;
    ol:queryLanguage ol:Sparql;
    ol:query '''
      PREFIX ex: <http://www.example.org/robots#> 
      SELECT ?mfgDate WHERE {
        ex:Robot_ABC ex:hasMfgDate ?mfgDate.
      }'''
  ];
  ol:targetFormat ol:JSON;
  ol:container "$.submodels[?(@.idShort=='Nameplate')].submodelElements[?(@.idShort=='DateOfManufacture')]";
  ol:snippet '{"value": "${?mfgDate}"}'.
  
  
<#SerialNoMapping> a ol:DataMap;
  ol:ontologicalSource [
    ol:source "robotConfig_ABC.ttl";
    ol:sourceType ol:File;
    ol:queryLanguage ol:Sparql;
    ol:query '''
      PREFIX ex: <http://www.example.org/robots#> 
      SELECT ?serialNo WHERE {
        ex:Robot_ABC ex:hasSerialNo ?serialNo.
      }'''
  ];
  ol:targetFormat ol:JSON;
  ol:container "$.submodels[?(@.idShort=='Nameplate')].submodelElements[?(@.idShort=='SerialNumber')]";
  ol:snippet '{"value": "${?serialNo}"}'.